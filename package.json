{
	"name": "bjcp-style-guidelines",
	"version": "1.0.0+20160531",
	"description": "BJCP Style Guidelines",
	"scripts": {
		"fmt": "prettier --write '**/*.{js,json,md}' '!package.json' '!.nyc_output/**/*'",
		"lint": "prettier --list-different '**/*.{js,json,md}' '!package.json' '!.nyc_output/**/*'",
		"test": "tap --node-arg=-r --node-arg=esm test.js"
	},
	"devDependencies": {
		"ajv": "6.x",
		"esm": "3.x",
		"prettier": "1.x",
		"tap": "13.x"
	}
}
